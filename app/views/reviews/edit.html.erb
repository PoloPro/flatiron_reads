<%= form_for (@review) do |f| %>

<div>
<%= f.label :book_id, "Book Title" %>
<%= f.text_field :book_id %>
</div>  

<div>
<%= f.label :rating %>
<%= f.text_field :rating %>
</div>

<div>
<%= f.label :description %>
<%= f.url_field :description %>
</div>  

<div>
<%= f.label :user_id, "User" %>
<%= f.text_field :user_id %>
</div>

<% f.submit %> 
<% end %>